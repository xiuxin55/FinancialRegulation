<local:BaseWindow x:Class="FinancialRegulation.UI.UserMaintenanceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:FinancialRegulation"
        xmlns:uc="clr-namespace:FinancialRegulation.UserCotrol"
        Title="用户管理" Height="300" Width="300" Background="{DynamicResource MyBrush}" Icon="/FinancialRegulation;component/Images/logo.png">
    <local:BaseWindow.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Template/ControlStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="../Template/ColorStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </local:BaseWindow.Resources>
    <Grid>
        <WindowsFormsHost Name="uwfh" Background="Transparent">
            <uc:UserMaintenance BackColor="Transparent"/>
        </WindowsFormsHost>
    </Grid>
</local:BaseWindow>
